<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="successhealth.SuccessHealth01.controller.Goals" displayBlock="true">
	<App xmlns="sap.m" id="idAppCont" height="100%" width="100%">
		<pages>
			<sap.m.semantic:MasterPage title="Health Goals" xmlns:sap.m.semantic="sap.m.semantic" id="page0" showNavButton="true"
				navButtonPress="onNavBack">
				<IconTabBar id="idIconTabBar" select="handleIconTabBarSelect" class="sapUiResponsiveContentPadding">
					<items>
						<IconTabFilter icon="sap-icon://goal" iconColor="Positive" count="0" text="Fitness Goals" key="Ok">
							<content>
								<VBox>
									<Image width="50em" height="10em" id="g1" src="/webapp/images/NoGoals.PNG" class="sapUiMediumMarginBottom"/>
								</VBox>
							</content>
						</IconTabFilter>
						<IconTabFilter icon="sap-icon://activity-assigned-to-goal" iconColor="Critical" count="0" text="Nutrition Goals" key="Heavy">
							<content>
								<VBox>
									<Image width="50em" height="10em" id="g2" src="/webapp/images/NoGoals.PNG" class="sapUiMediumMarginBottom"/>
								</VBox>
							</content>
						</IconTabFilter>
						<IconTabFilter icon="sap-icon://family-protection" iconColor="Negative" count="0" text="Happiness Goals" key="Overweight">
							<content>
								<VBox>
									<Image width="50em" height="10em" id="g3" src="/webapp/images/NoGoals.PNG" class="sapUiMediumMarginBottom"/>
								</VBox>
							</content>
						</IconTabFilter>
					</items>
				</IconTabBar>
				<sap.m.semantic:addAction>
					<sap.m.semantic:AddAction tooltip="Add" id="action0" press="onNavToHealthGoals"/>
				</sap.m.semantic:addAction>
			</sap.m.semantic:MasterPage>
		</pages>
	</App>
</mvc:View>